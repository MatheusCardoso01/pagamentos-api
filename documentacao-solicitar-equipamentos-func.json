{
  "openapi": "3.0.0",
  "info": {
    "title": "Solicitar Equipamentos e Gerar NF",
    "version": "1.0.0",
    "description": "API para solicitação de equipamentos e geração de Nota Fiscal."
  },
  "servers": [
    {
      "description": "Ambiente Dev",
      "url": "https://api-bus-dev.w4b.app"
    }
  ],
  "paths": {
    "/DomainFunctions/MatheusCardoso/2025_1/SolicitarEquipamentosEGerarNF": {
      "get": {
        "summary": "Solicitar equipamentos e gerar Nota Fiscal",
        "description": "Processa a solicitação e retorna os dados da Nota Fiscal gerada.",
        "responses": {
          "200": {
            "description": "Nota Fiscal gerada com sucesso",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/NotaFiscal"
                }
              }
            }
          },
          "500": {
            "description": "Erro interno ao gerar a Nota Fiscal"
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "NotaFiscal": {
        "type": "object",
        "properties": {
          "numeroNF": {
            "type": "string",
            "description": "Número da Nota Fiscal gerada",
            "example": "45821"
          },
          "dataChegadaEquipamentos": {
            "type": "string",
            "format": "date-time",
            "description": "Data prevista para chegada dos equipamentos",
            "example": "2025-12-05T10:30:00"
          }
        }
      }
    }
  }
}

